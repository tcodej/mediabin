{
  "name": "mediabin-ui",
  "private": true,
  "version": "1.0.0",
  "description": "A front end GUI for browsing my physical media collection served from the MediaBin server.",
  "type": "module",
  "scripts": {
    "dev": "stmux -w always -e ERROR -m beep,system -- [ [ \"vite\" : \"cd ../library-server && npm run dev\" ] ]",
    "dev-solo": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "vite build && node scripts/deploy.cjs"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "react-swipeable": "^7.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "ftp-deploy": "^2.4.6",
    "sass": "^1.70.0",
    "stmux": "^1.8.7",
    "vite": "^5.1.0"
  }
}
